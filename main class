package jogoDados; //a classe está implantada no pacote jogoDados
/*essa será a classe que permitirá que o programa rode*/
public class manjogoDados { //declaração da classe pública que está sendo manipulada
	public static void main(String[] args) { //declaração de uma função sem retorno com capacidade de armazenar strings e que chama a função principal
		//TODO Auto-generated method stub //definição de tarefas de programação do Eclipse
		jogoDados game = new jogoDados(); // instanciando dados (leitura de string) onde foi atribuido o valor de jogoDados a game 
		game.play(); //execução do método play() para a declaração game
	} //fim da da função public static void main(String[] args)
}//fim da classe manjogoDados
